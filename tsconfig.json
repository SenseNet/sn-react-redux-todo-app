{
    "files": [
        "src/main.tsx"
    ],
    "compilerOptions": {
        "target": "es2015",
        "module": "commonjs",
        "removeComments": true,
        "noImplicitAny": false,
        "sourceMap": true,
        "declaration": false,
        "preserveConstEnums": true,
        "experimentalDecorators": true,
        "typeRoots": [
            "./node_modules/@types"
        ],
        "jsx": "react",
        "outDir": "./dist/src"
    },
    "exclude": [
        "node_modules",
        "dist"
    ],
    "compileOnSave": false
}